{
  "name": "conversion-optimization",
  "version": "1.0.0",
  "description": "Complete conversion rate optimization workflow for Caf√© com Vendas landing page",
  "trigger": {
    "manual": ["User request for conversion improvement"],
    "automatic": {
      "condition": "conversion_rate < 2%",
      "frequency": "weekly"
    }
  },
  "phases": [
    {
      "phase": 1,
      "name": "Analysis & Discovery",
      "agents": [
        {
          "agent": "conversion-optimizer",
          "task": "Analyze current funnel performance and identify bottlenecks",
          "timeout": 300,
          "output": "funnel_analysis.json",
          "success_criteria": {
            "completeness": "all_pages_analyzed",
            "metrics_identified": true
          }
        },
        {
          "agent": "customer-insights",
          "task": "Map customer journey and identify friction points",
          "timeout": 300,
          "parallel": true,
          "output": "journey_map.json"
        }
      ]
    },
    {
      "phase": 2,
      "name": "Strategy Development",
      "agents": [
        {
          "agent": "market-researcher",
          "task": "Analyze competitor conversion strategies",
          "timeout": 240,
          "output": "competitor_analysis.json"
        },
        {
          "agent": "business-analyst",
          "task": "Define conversion KPIs and success metrics",
          "timeout": 180,
          "parallel": true,
          "output": "kpi_framework.json"
        }
      ]
    },
    {
      "phase": 3,
      "name": "Content Optimization",
      "agents": [
        {
          "agent": "angle-generator",
          "task": "Generate 3 strategic copy angles based on analysis",
          "timeout": 180,
          "output": "copy_angles.json"
        },
        {
          "agent": "portuguese-copywriter",
          "task": "Create optimized copy variants for A/B testing",
          "timeout": 300,
          "depends_on": ["angle-generator"],
          "output": "copy_variants.md"
        },
        {
          "agent": "copy-quality-specialist",
          "task": "Review and refine copy for clarity and impact",
          "timeout": 240,
          "depends_on": ["portuguese-copywriter"],
          "output": "refined_copy.md"
        }
      ]
    },
    {
      "phase": 4,
      "name": "Design Enhancement",
      "agents": [
        {
          "agent": "ui-designer",
          "task": "Create 3 CTA design variations",
          "timeout": 300,
          "output": "cta_designs.json"
        },
        {
          "agent": "ux-designer",
          "task": "Optimize user flow and interaction patterns",
          "timeout": 300,
          "parallel": true,
          "output": "ux_improvements.json"
        },
        {
          "agent": "design-compliance-specialist",
          "task": "Validate designs for accessibility and brand compliance",
          "timeout": 240,
          "depends_on": ["ui-designer", "ux-designer"],
          "output": "design_validation.json"
        }
      ]
    },
    {
      "phase": 5,
      "name": "Technical Implementation",
      "agents": [
        {
          "agent": "frontend-prototyper",
          "task": "Create HTML prototypes of top 3 variations",
          "timeout": 420,
          "output": "prototypes/*.html"
        },
        {
          "agent": "gtm-analytics-tracker",
          "task": "Set up conversion tracking and A/B test analytics",
          "timeout": 300,
          "parallel": true,
          "output": "tracking_setup.json"
        },
        {
          "agent": "performance-auditor",
          "task": "Ensure changes don't impact page speed",
          "timeout": 240,
          "depends_on": ["frontend-prototyper"],
          "output": "performance_report.json"
        }
      ]
    },
    {
      "phase": 6,
      "name": "Validation & Launch",
      "agents": [
        {
          "agent": "conversion-optimizer",
          "task": "Validate all changes and create test plan",
          "timeout": 300,
          "output": "test_plan.json"
        },
        {
          "agent": "orchestrator",
          "task": "Coordinate final review and launch sequence",
          "timeout": 180,
          "output": "launch_checklist.md"
        }
      ]
    }
  ],
  "success_metrics": {
    "primary": {
      "conversion_rate_improvement": ">20%",
      "statistical_significance": "p<0.05"
    },
    "secondary": {
      "bounce_rate_reduction": ">10%",
      "time_on_page_increase": ">15%",
      "lighthouse_score": ">90"
    }
  },
  "rollback_triggers": {
    "conversion_rate_drop": ">10%",
    "error_rate_spike": ">5%",
    "page_load_time_increase": ">2s"
  },
  "output": {
    "format": "markdown",
    "sections": [
      "executive_summary",
      "analysis_findings",
      "implemented_changes",
      "test_results",
      "recommendations"
    ],
    "artifacts": [
      "copy_variants.md",
      "design_prototypes/",
      "tracking_configuration.json",
      "performance_metrics.json"
    ]
  },
  "estimated_duration": "4-6 hours",
  "token_budget": 50000,
  "priority": "high",
  "tags": ["conversion", "optimization", "a/b-testing", "revenue"]
}